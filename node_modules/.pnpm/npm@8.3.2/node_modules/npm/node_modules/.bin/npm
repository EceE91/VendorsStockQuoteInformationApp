#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ece.ercan/EcesProjects/NN/AngularAspNetCoreApp/node_modules/.pnpm/npm@8.3.2/node_modules/npm/bin/node_modules:/Users/ece.ercan/EcesProjects/NN/AngularAspNetCoreApp/node_modules/.pnpm/npm@8.3.2/node_modules/npm/node_modules:/Users/ece.ercan/EcesProjects/NN/AngularAspNetCoreApp/node_modules/.pnpm/npm@8.3.2/node_modules:/Users/ece.ercan/EcesProjects/NN/AngularAspNetCoreApp/node_modules/.pnpm/node_modules:/Users/ece.ercan/EcesProjects/NN/AngularAspNetCoreApp/node_modules:/Users/ece.ercan/EcesProjects/NN/node_modules:/Users/ece.ercan/EcesProjects/node_modules:/Users/ece.ercan/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/ece.ercan/EcesProjects/NN/AngularAspNetCoreApp/node_modules/.pnpm/npm@8.3.2/node_modules/npm/bin/node_modules:/Users/ece.ercan/EcesProjects/NN/AngularAspNetCoreApp/node_modules/.pnpm/npm@8.3.2/node_modules/npm/node_modules:/Users/ece.ercan/EcesProjects/NN/AngularAspNetCoreApp/node_modules/.pnpm/npm@8.3.2/node_modules:/Users/ece.ercan/EcesProjects/NN/AngularAspNetCoreApp/node_modules/.pnpm/node_modules:/Users/ece.ercan/EcesProjects/NN/AngularAspNetCoreApp/node_modules:/Users/ece.ercan/EcesProjects/NN/node_modules:/Users/ece.ercan/EcesProjects/node_modules:/Users/ece.ercan/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/npm-cli.js" "$@"
else
  exec node  "$basedir/../../bin/npm-cli.js" "$@"
fi
